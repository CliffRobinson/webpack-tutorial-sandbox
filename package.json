{
    "name": "node-http-server",
    "version": "1.0.0",
    "description": "Node.js example server starter project",
    "scripts": {
        "start": "webpack && nodemon server",
        "knex": "knex",
        "test": "jest"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react",
            "babel-preset-jest"
        ],
        "plugins": [
            "import-directory",
            "@babel/plugin-transform-runtime"
        ]
    },
    "jest": {
        "testURL": "http://localhost/",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "setupFiles": [
            "./testSetup.js"
        ],
        "collectCoverage": true,
        "verbose": true
    },
    "dependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/runtime": "^7.9.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-import-directory": "^1.1.0",
        "babel-preset-jest": "^25.2.1",
        "body-parser": "^1.19.0",
        "core-js": "^3.6.4",
        "css-loader": "3.4.2",
        "csv-loader": "3.0.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "express": "4.17.1",
        "knex": "^0.20.13",
        "lodash": "4.17.15",
        "loglevel": "^1.6.7",
        "nock": "^12.0.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-saga": "^1.1.3",
        "redux-saga-test-plan": "^4.0.0-rc.3",
        "sqlite3": "^4.1.1",
        "style-loader": "1.1.3",
        "superagent": "^5.2.2",
        "webpack": "^4.42.1",
        "xml-loader": "1.2.1"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^3.0.0",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.0.3",
        "jest": "^25.2.7",
        "nodemon": "^2.0.2",
        "redux-devtools-extension": "^2.13.8",
        "supertest": "^4.0.2",
        "webpack-cli": "^3.3.10"
    },
    "keywords": []
}
